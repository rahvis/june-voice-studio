{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "POST"
      ],
      "route": "voice-enrollment"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "enrollmentQueue",
      "queueName": "voice-enrollment-queue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
