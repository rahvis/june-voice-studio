{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "POST"
      ],
      "route": "webhook/{webhook_type}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "notificationQueue",
      "queueName": "notification-queue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
